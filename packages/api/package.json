  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:cov": "vitest run --coverage",
    "test:debug": "vitest --inspect-brk --inspect --logHeapUsage --threads=false",
    "test:unit": "vitest run --project unit",
    "test:integration": "vitest run --project integration",
    "test:e2e": "vitest run --project e2e",
    "typeorm": "typeorm-ts-node-commonjs",
    "migration:generate": "pnpm build && pnpm typeorm migration:generate -d dist/data-source.js",
    "migration:run": "pnpm build && pnpm typeorm migration:run -d dist/data-source.js",
    "migration:revert": "pnpm build && pnpm typeorm migration:revert -d dist/data-source.js",
    "generate:openapi": "vitest run --project openapi"
  },
